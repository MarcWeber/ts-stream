{
	"extends": ["tslint:recommended", "tslint-config-prettier"],
	"rules": {
		"array-type": false,
		"interface-name": [true, "never-prefix"],
		"variable-name": [
			true,
			"ban-keywords",
			"check-format",
			"allow-pascal-case",
			"allow-leading-underscore"
		],
		"no-angle-bracket-type-assertion": false,
		"max-classes-per-file": false,
		"member-ordering": false
	}
}
